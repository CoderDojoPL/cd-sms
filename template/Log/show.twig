							<tr><th>Id</th>
								<th>Name</th>
								<th>Serial number</th>
								<th>State</th>
								<th>Location</th>
								<th>Warranty expiration date</th>
								<th>Price</th>
								<th>Note</th>
							</tr>
						</thead>
						<tbody>
							{%for record in addDevices%}
								<tr>
									<td>{{record.id}}</td>
									<td>{{record.name}}</td>
									<td>{{record.serial_number}}</td>
									<td>{{record.state}}</td>
									<td>{{record.location}}</td>
									<td>{{record.warranty_expiration_date|date('Y-m-d')}}</td>
									<td>{{record.price}}</td>
									<td>{{record.note}}</td>
								</tr>
							{%endfor%}
