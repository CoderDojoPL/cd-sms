<?xml version="1.0" ?>

<content xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">

    <resource pattern="\/css\/(.*)" expire="0" path="/css/{1}" />
    <resource pattern="\/js\/(.*)" expire="0" path="/js/{1}" />
    <resource pattern="\/font\/(.*)" expire="9999999" path="/font/{1}" />
    <resource pattern="\/uploaded\/device\/photo\/(.*)" expire="9999999" path="/uploaded/device/photo/{1}" />
    <resource pattern="\/favicon.ico" expire="9999999" path="/favicon.ico" />
</content>
